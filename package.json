  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' VITE_BACKEND_DEV=$VITE_BACKEND_DEV vite build",
    "build:ci": "NODE_OPTIONS='--max-old-space-size=8192' UV_THREADPOOL_SIZE=128 VITE_BACKEND_DEV=$VITE_BACKEND_DEV vite build",
    "build:mobile": "NODE_OPTIONS='--max-old-space-size=8192' VITE_BUILD_TARGET=mobile vite build",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "prettier": "prettier --write 'src/**/*.{js,jsx}'",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cap:ios": "npx cap run ios",
    "cap:android": "npx cap run android",
    "cap:build": "yarn build:mobile && npx cap copy",
    "cap:build:ios": "yarn build:mobile && npx cap copy ios && npx cap run ios",
    "cap:build:android": "yarn build:mobile && npx cap copy android && npx cap run android",
    "cap:sync": "npx cap sync",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android"
  },
